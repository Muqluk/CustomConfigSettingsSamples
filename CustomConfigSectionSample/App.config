<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="MyUrls" type="ConfigSectionCode.UrlsSection, CustomConfigSectionSample"/>
		<section name="SingleElementCollectionSample" type="ConfigSectionCode.SingleElementCollectionSection, CustomConfigSectionSample"/>
		<section name="FeatureTeamConfigurations" type="ConfigSectionCode.TeamEnvironmentConfigurationsSection, CustomConfigSectionSample"/>
	</configSections>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
	</startup>
	<MyUrls>
		<urls>
			<add name="first" url="http://yeahright.com" port="4040"/>
		</urls>
	</MyUrls>
	<SingleElementCollectionSample>
		<SingleCollection>
			<SingleElement Name="First Element Name" SampleStringValue1="First String Value"/>
			<SingleElement Name="Second Element Name" SampleStringValue1="Second String Value" SampleIntValue1="1"/>
			<SingleElement Name="Third Element Name" SampleStringValue1="Third String Value"/>
		</SingleCollection>
	</SingleElementCollectionSample>
	<FeatureTeamConfigurations>
		<FeatureTeams>
			<FeatureTeam Team="Cross Team Regression">
				<Environments>
					<Environment Name="Dev"></Environment>
					<Environment Name="QAF"></Environment>
					<Environment Name="QAP"></Environment>
					<Environment Name="Staging"></Environment>
				</Environments>
			</FeatureTeam>
			<FeatureTeam Team="DMX 15 Dashboard" SqlDatabaseSuffix="Documents" WebAppSuffix="docs">
				<Environments>
					<Environment Name="Dev Documents"></Environment>
					<Environment Name="QAF Documents"></Environment>
				</Environments>
			</FeatureTeam>
			<FeatureTeam Team="Swarm Web"></FeatureTeam>
		</FeatureTeams>
	</FeatureTeamConfigurations>
</configuration>